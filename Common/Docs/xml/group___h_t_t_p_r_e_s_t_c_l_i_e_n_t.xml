        <definition>Retcode_T HTTPRestClient_GetFirmware</definition>
        <argsstring>(HTTPRestClient_Config_T *config, HTTPRestClient_Get_T *get, uint32_t timeout)</argsstring>
        <name>HTTPRestClient_GetFirmware</name>
        <param>
          <type><ref refid="group___h_t_t_p_r_e_s_t_c_l_i_e_n_t_1ga9fcd7503395aa65821f564b870f68d57" kindref="member">HTTPRestClient_Config_T</ref> *</type>
