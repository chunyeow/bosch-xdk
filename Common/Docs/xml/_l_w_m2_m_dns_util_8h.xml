        <argsstring>(const char *const URL, char *const resolvedURL, const size_t sizeResolvedURL, uint32_t timeout, const LWM2MDnsUtil_Callback_T callback, void *context)</argsstring>
        <name>LWM2MDnsUtil_ExtendedResolveHostname</name>
        <param>
          <type>const char *const</type>
          <declname>URL</declname>
        </param>
        <param>
          <type>char *const</type>
          <declname>resolvedURL</declname>
