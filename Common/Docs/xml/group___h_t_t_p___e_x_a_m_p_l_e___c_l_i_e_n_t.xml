      <memberdef kind="function" id="group___h_t_t_p___e_x_a_m_p_l_e___c_l_i_e_n_t_1ga0d6ae41fbda1777d3e3b17ecb8aa7fc6" prot="public" static="yes" const="no" explicit="no" inline="no" virt="non-virtual">
